@ONT.PHYSICAL-OBJECT    = { IS-A @ONT.OBJECT; };
@ONT.SOCIAL-OBJECT      = { IS-A @ONT.OBJECT; };

@ONT.ATTRIBUTE          = { IS-A @ONT.PROPERTY; };
