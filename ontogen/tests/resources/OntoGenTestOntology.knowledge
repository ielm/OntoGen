@ONT.DESTINATION        = { IS-A @ONT.CASE-ROLE; INVERSE "destination-of"; };
@ONT.EXPERIENCER        = { IS-A @ONT.CASE-ROLE; INVERSE "experiencer-of"; };
@ONT.INSTRUMENT         = { IS-A @ONT.CASE-ROLE; INVERSE "instrument-of"; };
@ONT.LOCATION           = { IS-A @ONT.CASE-ROLE; INVERSE "location-of"; };
@ONT.PATH               = { IS-A @ONT.CASE-ROLE; INVERSE "path-of"; };
@ONT.SOURCE             = { IS-A @ONT.CASE-ROLE; INVERSE "source-of"; };
@ONT.THEME              = { IS-A @ONT.CASE-ROLE; INVERSE "theme-of"; };